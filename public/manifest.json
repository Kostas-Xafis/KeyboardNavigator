{
	"manifest_version": 3,
	"name": "Keyboard Navigator",
	"version": "1.0",
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content_scripts/script.js"]
		}
	],
	"permissions": ["tabs", "activeTab", "storage"],
	"action": {
		"default_popup": "src/popup/popup.html",
		"default_title": "Keyboard Navigator"
	}
}
